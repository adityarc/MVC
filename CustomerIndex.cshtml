@using tryingmvc.Models
@model List<Customer>


@{
    ViewBag.Title = "CustomerIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer Details</h2>

<table>
    <tr>
        <th>Customer ID </th>
        <th>Customer Name</th>
        <th>City</th>
        <th>Phone</th>
    </tr>
    @foreach (var Cust in Model.ToList())
    {
    <tr>
        <td> @Cust.CustomerID</td>
        <td> @Cust.CustomerName</td>
    
    
    
        <td> @Cust.City</td>
    
        <td> @Cust.Phone</td>
    </tr>
    }
</table>

